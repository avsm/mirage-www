#!/usr/bin/env bash
# Designed for the Travis CI build environment. See .travis.yml

set -ex
export OPAMYES=1
export OPAMJOBS=2

git -C /home/opam/opam-repository pull origin master
opam update
opam switch create 4.06.0
opam pin add jbuilder --dev
opam pin add -n rwo $TRAVIS_BUILD_DIR
opam install --deps-only rwo
